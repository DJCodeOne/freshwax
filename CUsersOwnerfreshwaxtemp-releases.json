   =================================== */

/* Container */
.admin-container {
  max-width: 1400px;
  margin: 0 auto;
  padding: 20px;
}

/* Loading State */
.admin-loading-container {
  display: flex;
  justify-content: center;
  align-items: center;
  min-height: 100vh;
  background: #ffffff;
}

.admin-loading-content {
  text-align: center;
}

/* Logo Container */
.admin-logo-container {
  background: white;
  padding: 20px;
  border-radius: 12px;
  margin-bottom: 20px;
  box-shadow: 0 2px 8px rgba(0,0,0,0.05);
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.admin-logo {
  height: 60px;
  width: auto;
}

/* Header Card */
.admin-header-card {
  background: white;
  padding: 30px;
  margin-bottom: 30px;
  border-radius: 12px;
  box-shadow: 0 2px 8px rgba(0,0,0,0.05);
}

.admin-header-card h1 {
  font-size: 36px;
  color: #000;
  margin-bottom: 10px;
}

.admin-header-card p {
  color: #000;
  font-size: 16px;
  font-weight: 400;
}

/* Stats Row */
.admin-stats-row {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 20px;
  margin-bottom: 30px;
}

.admin-stat-change {
  font-size: 12px;
  margin-top: 5px;
  font-weight: 500;
}

.admin-stat-change.positive {
  color: #28a745;
}

.admin-stat-change.negative {
  color: #dc3545;
}

.admin-stat-change.warning {
  color: #856404;
}

/* Quick Actions */
.admin-quick-actions {
  background: white;
  padding: 25px;
  border-radius: 12px;
  box-shadow: 0 5px 20px rgba(0,0,0,0.1);
  margin-bottom: 30px;
  border: 1px solid #e0e0e0;
}

.admin-quick-actions h2 {
  font-size: 20px;
  color: #000;
  margin-bottom: 20px;
  padding-bottom: 15px;
  border-bottom: 2px solid #e0e0e0;
}

.admin-action-buttons {
  display: flex;
  gap: 12px;
  flex-wrap: wrap;
}

.admin-action-btn {
  padding: 12px 24px;
  border: none;
  border-radius: 8px;
  cursor: pointer;
  font-size: 14px;
  font-weight: 600;
  transition: all 0.2s;
  text-decoration: none;
  display: inline-block;
}

.admin-action-btn:hover {
  transform: translateY(-2px);
  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);
}

.admin-action-btn.primary {
  background: #000;
  color: white;
}

.admin-action-btn.secondary {
  background: #666;
  color: white;
}

.admin-action-btn.success {
  background: #28a745;
  color: white;
}

.admin-action-btn.warning {
  background: #ffc107;
  color: #000;
}

/* Modules Grid */
.admin-modules-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
  gap: 25px;
  margin-bottom: 30px;
}

.admin-module-card {
  background: white;
  border-radius: 12px;
  padding: 30px;
  box-shadow: 0 5px 20px rgba(0,0,0,0.1);
  transition: all 0.3s;
  cursor: pointer;
  position: relative;
  overflow: hidden;
  border: 1px solid #e0e0e0;
}

.admin-module-card::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 4px;
  background: #000;
}

.admin-module-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 30px rgba(0,0,0,0.15);
  border-color: #000;
}

.admin-module-icon {
  font-size: 48px;
  margin-bottom: 15px;
}

.admin-module-title {
  font-size: 20px;
  font-weight: 700;
  color: #000;
  margin-bottom: 10px;
}

.admin-module-description {
  font-size: 14px;
  color: #666;
  line-height: 1.6;
  margin-bottom: 15px;
}

.admin-module-badge {
  display: inline-block;
  padding: 4px 12px;
  background: #f5f5f5;
  color: #000;
  border-radius: 12px;
  font-size: 12px;
  font-weight: 600;
  margin-top: 10px;
  border: 1px solid #e0e0e0;
}

.admin-module-badge.new {
  background: #d4edda;
  color: #28a745;
  border-color: #28a745;
}

.admin-module-badge.important {
  background: #fff3cd;
  color: #856404;
  border-color: #ffc107;
}

/* Recent Activity */
.admin-recent-activity {
  background: white;
  padding: 25px;
  border-radius: 12px;
  box-shadow: 0 5px 20px rgba(0,0,0,0.1);
  border: 1px solid #e0e0e0;
}

.admin-recent-activity h2 {
  font-size: 20px;
  color: #000;
  margin-bottom: 20px;
  padding-bottom: 15px;
  border-bottom: 2px solid #e0e0e0;
}

.admin-activity-item {
  padding: 15px;
  border-left: 3px solid #000;
  background: #f9f9f9;
  border-radius: 6px;
  margin-bottom: 12px;
}

.admin-activity-time {
  font-size: 12px;
  color: #666;
  margin-bottom: 5px;
}

.admin-activity-text {
  font-size: 14px;
  color: #000;
}

